<script>    
    $(document).ready(function() {
        
        //Enhanced Ecommerce
        
        var opns = [];
        var wpn = $('#productOverviewTable').data('wpn');
        var ecommerceTaxonomy = $('#productOverviewTable').data('ecommtax');    
    
        $('#productOverviewTable  tr.prodListItem, #productOverviewTable  tr.prodListItem2').each(function() {
            var opn = $(this).data('opn');
            var shortDesc = $(this).data('shortdesc');
            var price = $(this).data('price');
            var packageType = $(this).data('packagetype');
            var containerType = $(this).data('containertype');
            var productStatus = $(this).data('productstatus');
            var unitQty = new Number($(this).data('unitqty'));
            
            var distributors = 'n/a';            
            if ($('#inv_' + opn + ' td.distributorName').length) {
                var distArr = [];
                $('#inv_' + opn + ' td.distributorName').each(function() {
                    distArr.push($(this).data('distname'));
                });
                distributors = distArr.join('|');
            }
            
            var cta = getCta(this);
            
            opns.push([{
                'id': wpn, 
                'name': shortDesc,  
                'price': price,  
                'category': ecommerceTaxonomy, 
                'variant': opn, 
                'dimension15': getCustDimVal(packageType),  
                'dimension16': getCustDimVal(containerType),  
                'dimension17': distributors,  
                'dimension18': getCustDimVal(productStatus), 
                'metric2': getCustMetricVal(unitQty), 
                'metric3': cta
                 }
            ]);
        });
        
        var evalBoards = [];
        $('#evalboardTable .evalBoardItem').each(function() {
            var evbId = $(this).data('id');
            var evbName = $(this).data('name');
            var price = $(this).data('price');
            var usedOpns = $(this).data('usedopns');
            var position = $(this).data('position');
            var evbstatus = $(this).data('evbstatus');            
            var cta = getCta(this);
            
            evalBoards.push({
                'id': evbId,
                'name': evbName,
                'price': getGreaterThanZero(price),
                'category': ecommerceTaxonomy,
                'variant': usedOpns,
                'list': 'Product Overview Page',
                'position': position,
                'metric3': cta,
                'dimension18': getCustDimVal(evbstatus)
             });
        });
        
        dataLayer.push({
            'ecommerce' : {
                'currencyCode': 'USD',
                'detail' : {
                    'products': opns
                },
                'impressions' : evalBoards
            },
            'event': 'ecommerce'
        });
    });
    
    function getCta(parentElement) {
        var cta = 0;
        if ($(parentElement).find('.clickThru').length) {
            cta += 1;
        }
        if ($(parentElement).find('.salesSupport').length) {
            cta += 10;
        }
        if ($(parentElement).find('.inventoryCheck').length) {
            cta += 100;
        }
        if ($(parentElement).find('.samplesProvided').length) {
            cta += 1000;
        }
        return cta;
    }
    
</script>
